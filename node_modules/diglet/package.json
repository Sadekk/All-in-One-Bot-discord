{
  "name": "diglet",
  "version": "1.0.6",
  "description": "secure http tunnelling server and client",
  "main": "index.js",
  "bin": {
    "diglet": "bin/diglet.js"
  },
  "scripts": {
    "test": "mocha",
    "linter": "jshint --config .jshintrc",
    "coverage": "istanbul cover _mocha",
    "build-docs": "mkdir -p ./jsdoc && rm -r ./jsdoc && jsdoc lib -r -R README.md -c .jsdoc.json --verbose -d ./jsdoc",
    "deploy-docs": "gh-pages -d jsdoc --repo git@github.com:bookchin/diglet.git"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bookchin/diglet.git"
  },
  "keywords": [
    "tunnel",
    "https"
  ],
  "author": "Gordon Hall <gordon@gordonwritescode.com>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/bookchin/diglet/issues"
  },
  "homepage": "https://github.com/bookchin/diglet#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "gh-pages": "^0.11.0",
    "ink-docstrap": "boshevski/docstrap",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "jshint": "^2.9.4",
    "mocha": "^2.5.3",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6",
    "ws": "^1.1.1"
  },
  "dependencies": {
    "bunyan": "^1.8.5",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "portastic": "^1.0.1",
    "rc": "^1.1.6",
    "serve-static": "^1.11.1",
    "through": "^2.3.8",
    "tldjs": "^1.7.0"
  }
}
